{
    "mysql+pymysql": {
        "tables": "show tables;",
        "table_ops": {
            "count": "select count(*) from $var$;",
            "rows": "select * from $var$ limit 0,100;"
        }
    },
    "mssql+pymssql": {
        "tables": "select name from sys.tables",
        "table_ops": {
            "count": "select count(*) from $var$;",
            "rows": "select top 100 * from $var$;"
        }
    },
    "oracle": {
        "tables": "show tables;",
